{
  "name": "ticketerbot",
  "version": "4.0.0",
  "description": "Ticketer Bot rewritten in TS with DiscordJS and DiscordAkairo",
  "main": "app.ts",
  "scripts": {
    "build": "npm run lint && rm -rf dist && tsc",
    "build:dev": "rm -rf dist && tsc",
    "dev": "npm run build:dev && node dist/app.js NODE_ENV=development",
    "prod": "npm run build && node dist/app.js NODE_ENV=production",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "author": "twist#7777",
  "repository": {
    "type": "git",
    "url": "https://github.com/dtavana/TicketerBotV4.git"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "discord-akairo": "1Computer1/discord-akairo",
    "discord.js": "discordjs/discord.js",
    "js-yaml": "^3.13.1",
    "mongoose": "^5.9.10",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/mongoose": "^5.7.14",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  }
}
